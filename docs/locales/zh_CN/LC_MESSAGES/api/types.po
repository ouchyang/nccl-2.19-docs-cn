#: ../../source/api/types.rst:131 26aac38de0934b66b23bfae76d775120
msgid "ncclRedOp_t"
msgstr "ncclRedOp_t"

#: ../../source/api/types.rst:135 e905113fe99c45d5b06bf151e38b5bc1
msgid "Defines the reduction operation."
msgstr "定义了减少操作。"

#: ../../source/api/types.rst:139 8f609e3176c3491b90c66c5a4e8d7c52
msgid "Perform a sum (+) operation"
msgstr "执行一个求和（+）操作"

#: ../../source/api/types.rst:143 90463cef4b30440faef525b6c233800d
msgid "Perform a product (*) operation"
msgstr "执行一个产品 (*) 操作"

#: ../../source/api/types.rst:147 1fd58161f09640f3a0a28f304d63fe54
msgid "Perform a min operation"
msgstr "执行一个 min 操作"

#: ../../source/api/types.rst:151 650608ddb1ec4c64ae2565bbd6c244f7
msgid "Perform a max operation"
msgstr "执行最大值操作"

#: ../../source/api/types.rst:155 b9dba90eae9b47b7abf365569e708fcd
msgid "Perform an average operation, i.e. a sum across all ranks, divided by the number of ranks."
msgstr "执行平均操作，即在所有等级上求和，然后除以等级数。"

#: ../../source/api/types.rst:159 9af5fa170f7c4d9486fb42c4a5542650
